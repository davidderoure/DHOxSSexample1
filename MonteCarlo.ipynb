    "import random\n",
    "\n",
    "count = 0\n",
    "\n",
    "for i in range(1,100):\n",
    "    x = random.uniform(-1, 1)\n",
    "    y = random.uniform(-1, 1)\n",
    "\n",
    "    if x**2 + y**2 <= 1:\n",
    "        count += 1\n",
    "\n",
    "    print(4.0 * count / i, end = ' ')"
   ]
